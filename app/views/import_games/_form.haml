= form_for @game do |f|
  - if @game.errors.any?
    #error_explanation
      %h2= "#{pluralize(@game.errors.count, "error")} prohibited this game from being saved:"
      %ul
        - @game.errors.full_messages.each do |msg|
          %li= msg

  .field
    =# f.label :game_logs
    = f.file_field :game_logs, multiple: true
  .actions
    = f.submit 'Save'